{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["esnext","dom"],
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "src/swagger-output.json"
  ],
  "exclude": ["node_modules"]
}
